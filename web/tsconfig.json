{
  "extends": "../tsconfig.json",
  "include": ["**/*.tsx", "**/*.ts", "**/*.js", "**/*.jsx"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "react-jsx",
    "lib": ["ESNext", "DOM"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "resolveJsonModule": false,
    "target": "ESNext",
    "types": ["vite/client"]
  }
}
